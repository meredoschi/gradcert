<div class="row">
		<table>
			<tr>
			
							<div class="large-4 columns">
					
								<th><strong><%= tm('address').capitalize %></strong></th>
					
							</div>
					
							
			</tr>						

		
			<div class="row">
		
				<tr>
					<div class="large-12 columns">
					
						<td><%= @course.address.header %></td>
					
					</div>
										
				</tr>
		</div>		
		
		
			<div class="row">
		
				<tr>
					
						<td><%= @course.address.street %> - <%= @course.address.neighborhood %></td>
					
										
				</tr>
				
				<tr>
					
						<td><%= @course.address.municipality.name %></td>
					
										
				</tr>

				<tr>
					
						<td><%= ta('address.postalcode') %> <%= @course.address.postalcode %></td>
					
					
					</div>
										
				</tr>
				
		</div>

	</table>
</div>