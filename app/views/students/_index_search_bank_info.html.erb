
<div class="row custom-panel6">

  <div class="large-3 columns">

    <div class="field">

      <th><%= f.label ta('bankaccount.num')+' '+tm('bankaccount').downcase %></th>
      <span data-tooltip aria-haspopup="true" class="has-tip" title="Aqui é possível, caso necessário, consultar se um número de conta bancária (sem o dígito verificador) já consta do sistema.  O cadastro de números de conta iguais é permitido somente se as agências forem distintas."><%= t('optional')+' ('+t('help')+')' %></span>

    </div>

  </div>

  <div class="large-3 columns">

      <%= f.text_field :bankaccount_num_cont %>

  </div>

  <div class="large-3 columns">

    <div class="field">

      <th><%= f.label tm('bankbranch')%></th>

    </div>

  </div>

  <div class="large-3 columns">

      <%= f.text_field :bankaccount_bankbranch_code_cont %>

  </div>

</div>

