<div class="row">

	<table>

		<tr>

			<div class="row">

				<div class="large-4 columns">

					<th><strong><%= ta('admission.candidates')%></strong></th>

				</div>

				<div class="large-4 columns">

					<th><strong><%= ta('admission.absentfirstexam') %></strong></th>

				</div>

				<div class="large-4 columns">

					<th><strong><%= ta('admission.passedfirstexam')%></strong></th>

				</div>

			</div>

		</tr>

		<div class="row">

		<tr>

			<div class="large-4 columns">
				<td><%=	admission.candidates %></td>
			</div>

			<div class="large-4 columns">

					<td><%=	admission.absentfirstexam %></td>

			</div>

			<div class="large-4 columns">

					<td><%=	admission.passedfirstexam %></td>
			</div>

		</div>
	</table>

</div>


<div class="row">

	<table>

		<tr>

			<div class="row">

				<div class="large-4 columns">

					<th><strong><%= ta('admission.absentfinalexam')%></strong></th>

				</div>

				<div class="large-4 columns">

					<th><strong><%= ta('admission.appealsgrantedfinalexam')%></strong></th>

				</div>

				<div class="large-4 columns">

					<th><strong><%= ta('admission.appealsdeniedfinalexam')%></strong></th>

				</div>


			</div>

		</tr>

		<div class="row">

		<tr>

			<div class="large-4 columns">
				<td><%=	admission.absentfinalexam %></td>
			</div>

			<div class="large-4 columns">
				<td><%=	admission.appealsgrantedfinalexam %></td>
			</div>

			<div class="large-4 columns">
				<td><%=	admission.appealsdeniedfinalexam %></td>
			</div>

		</div>
	</table>

</div>

<div class="row">

	<table>

		<tr>

			<div class="row">

				<div class="large-3 columns">

					<th><strong><%= ta('admission.admitted')%></strong></th>

				</div>

				<div class="large-3 columns">

					<th><strong><%= ta('admission.convoked')%></strong></th>

				</div>

				<div class="large-3 columns">

				<th><strong><%#= t('approval_rate')%></strong></th>

			</div>

			<div class="large-3 columns">

				<th><strong><%#= t('convocation_rate')%></strong></th>

			</div>


			</div>

		</tr>

		<div class="row">

		<tr>
			<div class="large-3 columns">
				<td><%=	admission.admitted %></td>
			</div>

			<div class="large-3 columns">
				<td><%=	admission.convoked %></td>
			</div>

		<div class="large-3 columns">
			<td><%#= percent(admission.admitted, admission.candidates) %></td>
		</div>


		<div class="large-3 columns">
			<td><%#= percent(admission.convoked, admission.candidates) %></td>
		</div>

		</div>
	</table>

</div>
