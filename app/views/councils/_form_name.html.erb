<div class="row panel callout">

	<div class="row">

		 		<div class="large-2 columns", class: "left inline">

			 	 <%= f.label ta('council.name')%> <br>

				</div>

		 		<div class="large-10 columns">

			 	  <%= f.text_field :name %>

				</div>

	</div>

	<div class="row">

		 		<div class="large-2 columns", class: "left inline">

			 	 <%= f.label ta('council.abbreviation')%> <br>

				</div>

		 		<div class="large-4 columns", class: "left inline">

			 	  <%= f.text_field :abbreviation %>

				</div>


		 		<div class="large-2 columns", class: "left inline">

			 	 <%= f.label ta('council.state_id')%> <br>

				</div>

		 		<div class="large-4 columns", class: "left inline">

			  	<%#= f.collection_select :state_id, State.all, :id, :name,  {prompt: true} %>
					<%= f.collection_select :state_id, State.home, :id, :name %>

					<%#= f.collection_select :state_id, @homestate, :id, :name,  {prompt: true}, selected: @homestate_id  %>

				</div>


	</div>

</div>
