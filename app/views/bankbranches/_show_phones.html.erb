<% if @bankbranch.phone.present? %>

		<div class="row">
				<table>
					<tr>
			
						<% if @bankbranch.phone.main.present? %>
		
								<div class="large-4 columns">
								
										<th><strong><%= ta('contact.phone')%></strong></th>
								
								</div>
			
						<% end %>
					
						<% if @bankbranch.phone.other.present? %>
		
							<div class="large-4 columns">
								
										<th><strong><%= ta('contact.other')%></strong></th>
								
							</div>
			
							<% end %>
		
						<% if @bankbranch.phone.mobile.present? %>
		
							<div class="large-4 columns">
								
										<th><strong><%= ta('contact.mobile')%></strong></th>
								
							</div>
			
							<% end %>
											
					</tr>						
				
					<div class="row">
				
						<tr>
						
							<% if @bankbranch.phone.main.present? %>
		
								<div class="large-4 columns">
								
									<td> <%= @bankbranch.phone.main %> </td>
								
								</div>
							
							<% end %>
							
							<% if @bankbranch.phone.other.present? %>
		
								<div class="large-4 columns">
								
									<td> <%= @bankbranch.phone.other %> </td>
								
								</div>
				
							<% end %>				
		
							<% if @bankbranch.phone.mobile.present? %>
		
								<div class="large-4 columns">
								
									<td> <%= @bankbranch.phone.mobile %> </td>
								
								</div>
				
							<% end %>				
										
					</tr>
				</div>
			</table>
		</div>
		
<% end %>