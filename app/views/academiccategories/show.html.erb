<div class="row">

	<h2><%= tm('academiccategory').capitalize %></h2>

</div>


<div class="row">
		<table>
			<tr>
			
							<div class="large-4 columns">
					
								<th><strong><%= ta('academiccategory.name')%></strong></th>
					
							</div>
					
							
			</tr>						
		
			<div class="row">
		
				<tr>
					<div class="large-12 columns">
					
						<td><%= @academiccategory.name %></td>
					
					</div>
										
				</tr>
		</div>

	</table>
</div>


<div class="row">
	
	<% if can? :edit, @academiccategory %>

		<%= link_to t('actions.edit'), edit_academiccategory_path(@academiccategory), class: t('edit')  %> |

	<% end %>
	
	<%= link_to t('actions.back'), academiccategories_path,  class: 'voltar'  %>

</div>