<div class="row">
	
	<h3> <%= pluralize(@taxation.brackets.count, tm('bracket'))%> (<%= currency_symbol %>)  </h3>

</div>

<% @taxation.brackets.each do |bracket| %>

<div class="row">
	<table>
	
		<tr>

			<div class="row">

				<div class="large-2 columns">
		
					<th><strong><%= Pretty.ordinalize_feminine(bracket.num) %></strong></th>
		
				</div>
					
				<div class="large-2 columns">
		
					<th><strong><%= ta('bracket.start')%></strong></th>
		
				</div>
		
				<div class="large-2 columns">
		
					<th><strong><%= ta('bracket.finish')%></strong></th>
		
				</div>

				<div class="large-2 columns">

					<th><strong><%= ta('bracket.rate')%></strong></th>		
		
				</div>

				<div class="large-2 columns">
		
					<th><strong><%= ta('bracket.deductible')%></strong></th>		
		
				</div>
							
			</div>
	
		</tr>

		<div class="row">

		<tr>
	
			<div class="large-2 columns">
			</div>
				<td></td>	
			<div class="large-2 columns">
				<td><%=	humanized_money(bracket.start) %></td>
			</div>

			<div class="large-2 columns">
				<td><%=	display_limit(bracket) %></td>
			</div>

			<div class="large-2 columns">
				<td><%=	bracket.rate %></td>
			</div>

			<div class="large-2 columns">
				<td><%=	humanized_money(bracket.deductible) %></td>
			</div>
																						
		</div>
	</table>
	</div>
</div>	
<% end %>
	