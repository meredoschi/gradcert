<div class="row">
		
	<tr>
	
			<div class="large-6 columns">
		
					<td><%= ta('funding.government').capitalize %></td>
		
				</div>
	
				<div class="large-3 columns">
		
					<td><%= @characteristic.funding.government %></td>
		
				</div>
	
				<% if @characteristic.funding.absolute? %>
							
					<div class="large-3 columns">
			
						<th><%= percent(@characteristic.funding.government, @characteristic.funding.total) %></th>
		
					</div>

				<% end %>

		</div>
	 			 		
	</tr>

</div>	