<%= search_form_for @search do |f| %>

	<div class="row">

		<div class="large-3 columns">
		
			<div class="field">
			
			  <th><%= f.label ta('methodology.kind')%></th>
		
			</div>

		</div>
			
		<div class="large-9 columns">
		
				<%= f.text_field :kind_cont %>  
		
		</div>
	    
	</div>


	<div class="row">
		    
		<div class="large-6 columns">
	
		  <%= f.submit t('actions.search'), class: "buscar" %>
	
		</div>
	
	</div>
	 
<% end %>